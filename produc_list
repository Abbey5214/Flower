<script>
    var page=document.getElementById("page")
    var as=page.getElementsByTagName("a")
    for(var a of as){
        a.onclick=function(){
            var a=this;
            lable=a.parentNode.children[0];
            var n=parseInt(lable.innerHTML);
            //console.log(n);
            if (a.textContent==">"){
                n+=1;
               // console.log(a.innerHTML);
            }else if(n>1){
                n--;
            }
            lable.innerHTML=n;
        }
    }


    var userClick=document.querySelector("div.rt_turn>a");
    console.log(userClick)
    userClick.onclick=function(){
        var d2=this;
        var d1=document.querySelector("div.click_close");
        console.log(d2.textContent)
        //d1.style.display="none";
        if(d2.textContent=="收起"){
            d1.style.display="none";
            d2.innerHTML="展开";
        }else{
            d1.style.display="block";
            d2.innerHTML="收起";
        }
    }
</script>
